	xsi:schemaLocation="http://www.solmix.org/xmlns/datasource/v1.0.1 http://www.solmix.org/xmlns/datasource/v1.0.1 ">
	<DataSource ID="mybatis" serverType="mybatis" dbName="solmix">
		<operationBindings>
			<operationBinding operationType="fetch" operationId="fetch">
				<queryClauses>
					<customQL>org.solmix.test.mybatis.Menu.getMenu</customQL>
				</queryClauses>
			</operationBinding>
			<operationBinding operationType="add" operationId="add">
				<queryClauses>
					<customQL>org.solmix.test.mybatis.Menu.insertMenu</customQL>
				</queryClauses>
			</operationBinding>
			<operationBinding operationType="update" operationId="update">
