<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Solmix'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.google.gwt.uibinder.UiBinder" />
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name="com.google.gwt.user.theme.standard.Standard" />
	<!-- google chart api for gwt -->
	<inherits name='com.google.gwt.visualization.Visualization' />
	<inherits name="com.smartgwt.SmartGwtNoScript" />
	<inherits name="com.solmix.gwt.theme.normal.Normal" />
	<inherits name="com.solmix.gwt.theme.normal.NormalResources" />
	<!-- Other module inherits -->
	<!-- atmosphere comet -->
	<inherits name="org.solmix.comet.gwt.Client" />
	<!-- mvp -->
	<inherits name="com.gwtplatform.mvp.Mvp" />
	<inherits name="com.gwtplatform.dispatch.Dispatch" />
	<!-- ofcgwt chart -->
	<inherits name="ua.metallic.ofcchart.OFC4SGWT" />

	<!-- Other module inherits -->
	<!-- For development a default of `DEBUG` is recommended -->
	<!-- <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" /> -->
	<!-- For production most teams prefer to set the default log level to `OFF` -->
	<inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />

	<!-- In gwt-log-3.0.3 or later -->
	<!-- <inherits name="com.allen_sauer.gwt.log.gwt-log-RemoteLogger" /> -->

	<!-- Compile Logger at `DEBUG` level -->
	<extend-property name="log_level" values="DEBUG" />
	<set-property name="log_level" value="DEBUG" />


	<!-- Specify the app entry point class. -->
	<entry-point class='org.solmix.web.client.Solmix' />
	<!--chinese / china -->
	<extend-property name="locale" values="zh_CN" />
	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	<define-configuration-property name="gin.ginjector"
		is-multi-valued="false" />
	<set-configuration-property name="gin.ginjector"
		value="org.solmix.web.client.gin.SolmixGinjector" />

</module>
