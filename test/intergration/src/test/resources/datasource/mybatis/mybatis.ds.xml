				</queryClauses>
			</operationBinding>
			<operationBinding operationType="remove" operationId="remove">
				<queryClauses>
					<customQL>org.solmix.test.mybatis.Menu.deleteMenu</customQL>
				</queryClauses>
			</operationBinding>
		</operationBindings>
